{
    "name": "@geometryzen/esprima",
    "version": "0.9.9",
    "description": "ECMAScript parsing infrastructure for multipurpose analysis",
    "homepage": "http://esprima.org",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "system": "./dist/system/index.min.js",
            "default": "./dist/commonjs/index.js"
        }
    },
    "main": "./dist/commonjs/index.js",
    "module": "./dist/esm/index.min.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/commonjs/index.js",
        "dist/commonjs/index.js.map",
        "dist/esm/index.min.js",
        "dist/esm/index.min.js.map",
        "dist/index.d.ts",
        "dist/system/index.min.js",
        "dist/system/index.min.js.map"
    ],
    "bin": {
        "esparse": "./bin/esparse.js",
        "esvalidate": "./bin/esvalidate.js"
    },
    "engines": {
        "node": ">=8"
    },
    "author": {
        "name": "Ariya Hidayat",
        "email": "ariya.hidayat@gmail.com"
    },
    "maintainers": [
        {
            "name": "Ariya Hidayat",
            "email": "ariya.hidayat@gmail.com",
            "web": "http://ariya.ofilabs.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jquery/esprima.git"
    },
    "bugs": {
        "url": "https://github.com/jquery/esprima/issues"
    },
    "license": "BSD-2-Clause",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@skypack/package-check": "^0.2.2",
        "@types/jest": "^29.5.12",
        "@types/rollup-plugin-peer-deps-external": "^2.2.5",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "codecov.io": "~0.1.6",
        "eslint": "^8.57.0",
        "eslint-plugin-esprima-internal": "file:eslint",
        "everything.js": "~1.0.3",
        "glob": "~10.3.10",
        "gh-pages": "^6.1.1",
        "istanbul": "~0.4.5",
        "jest": "^29.7.0",
        "json-diff": "~1.0.6",
        "mocha": "~10.3.0",
        "node-tick-processor": "~0.0.2",
        "regenerate": "~1.4.2",
        "release-it": "^17.1.1",
        "results-interpreter": "~1.0.0",
        "rollup": "^4.12.0",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "temp": "~0.9.4",
        "test262-stream": "~1.4.0",
        "ts-jest": "^29.1.2",
        "ts-jest-resolver": "^2.0.1",
        "ts-node": "^10.9.2",
        "typedoc": "^0.25.10",
        "typescript": "~5.3.3",
        "typescript-formatter": "~7.2.2",
        "unicode-12.1.0": "^0.8.0",
        "webpack": "~1.14.0"
    },
    "keywords": [
        "ast",
        "ecmascript",
        "esprima",
        "javascript",
        "parser",
        "syntax"
    ],
    "scripts": {
        "build": "npm run clean && rollup --config rollup.config.mts --configPlugin @rollup/plugin-typescript",
        "check-version": "node test/check-version.js",
        "docs": "npm run docs:typedoc && open-cli docs/index.html",
        "docs:typedoc": "rm -rf docs && typedoc --options typedoc.json",
        "lint": "eslint src/*.ts",
        "pages": "npm run docs:typedoc && gh-pages -d docs",
        "pretest": "npm run compile",
        "prepublishOnly": "npm run check && npm run build && npm run test && npm run lint && npm run pages",
        "release": "release-it",
        "jest": "npm run build && jest",
        "lint-code": "node test/check-version.js",
        "code-style": "tsfmt --verify src/*.ts && tsfmt --verify test/*.js",
        "format-code": "tsfmt -r src/*.ts",
        "format-test": "tsfmt -r test/benchmark-parser.js && tsfmt -r test/benchmark-tokenizer.js",
        "static-analysis": "npm run check-version && npm run lint-code && npm run code-style",
        "hostile-env-tests": "node test/hostile-environment-tests.js",
        "unit-tests": "node test/unit-tests.js",
        "api-tests": "mocha -R dot test/api-tests.js",
        "grammar-tests": "node test/grammar-tests.js",
        "regression-tests": "node test/regression-tests.js",
        "test-262": "npm install git+https://git@github.com/tc39/test262.git#36d2d2d && node test/test-262.js",
        "all-tests": "npm run verify-line-ending && npm run generate-fixtures && npm run unit-tests && npm run api-tests && npm run grammar-tests && npm run regression-tests && npm run hostile-env-tests",
        "verify-line-ending": "node test/verify-line-ending.js",
        "generate-fixtures": "node tools/generate-fixtures.js",
        "analyze-coverage": "istanbul cover test/unit-tests.js",
        "check-coverage": "istanbul check-coverage --statement 99.44 --branch 98.87 --function 97.88 --lines 99.52",
        "dynamic-analysis": "npm run analyze-coverage && npm run check-coverage",
        "clean": "rm -rf coverage && rm -rf dist && rm -rf docs && rm -rf es2015 && rm -rf system && rm -rf types && rm -rf build && rm -rf src/*.js",
        "compile": "tsc -p src/ && webpack && node tools/fixupbundle.js",
        "test": "npm run compile && npm run all-tests && npm run static-analysis && npm run dynamic-analysis",
        "prepublish": "npm run build",
        "profile": "node --prof test/profile.js && mv isolate*.log v8.log && node-tick-processor",
        "benchmark-parser": "node -expose_gc test/benchmark-parser.js",
        "benchmark-tokenizer": "node --expose_gc test/benchmark-tokenizer.js",
        "benchmark": "npm run benchmark-parser && npm run benchmark-tokenizer",
        "codecov": "istanbul report cobertura && codecov < ./coverage/cobertura-coverage.xml",
        "downstream": "node test/downstream.js",
        "generate-regex": "node tools/generate-identifier-regex.js",
        "generate-xhtml-entities": "node tools/generate-xhtml-entities.js"
    }
}
